{  "run_list" : [    "recipe[apt]",    "recipe[simple_iptables]",    "recipe[iptables_basic]",    "recipe[user_base]",    "recipe[user]",    "recipe[user::data_bag]",    "recipe[sudo]",    "recipe[openssh]",    "recipe[base]",    "recipe[vim]",    "recipe[nginx]",    "recipe[mysql::server]",    "recipe[database::mysql]",    "recipe[php]",    "recipe[php-fpm]",    "recipe[server_base]",    "recipe[gitlab]"    ],  "_comment_users_setting" : [ "readme file : ./cookbooks/user/README.md" ],  "users" : ["kitsunai"],  "_comment_sudo_setting" : [ "readme file :  ./cookbooks/sudo/README.md"],  "authorization" : {    "sudo" : {      "groups"       : ["admin", "wheel", "sysadmin"],      "users"        : ["kitsunai"],      "passwordless" : "true"    }  },  "_comment_openssh_setting" : [ "readme file : ./cookbooks/openssh/README.md"],  "openssh" : {    "client" : {      "*" : {        "send_env"                         : "LANG LC_*",        "hash_known_hosts"                 : "yes",        "g_s_s_a_p_i_authentication"       : "yes",        "g_s_s_a_p_i_delegate_credentials" : "no"      }    },    "server" : {      "password_authentication"           : "yes",      "port"                              : "22",      "protocol"                          : "2",      "host_key"                          : ["/etc/ssh/ssh_host_rsa_key", "/etc/ssh/ssh_host_dsa_key", "/etc/ssh/ssh_host_ecdsa_key"],      "use_privilege_separation"          : "yes",      "key_regeneration_interval"         : "3600",      "server_key_bits"                   : "768",      "syslog_facility"                   : "AUTH",      "log_level"                         : "INFO",      "login_grace_time"                  : "120",      "permit_root_login"                 : "yes",      "strict_modes"                      : "yes",      "r_s_a_authentication"              : "yes",      "pubkey_authentication"             : "yes",      "ignore_rhosts"                     : "yes",      "rhosts_r_s_a_authentication"       : "no",      "hostbased_authentication"          : "no",      "permit_empty_passwords"            : "no",      "challenge_response_authentication" : "no",      "x11_forwarding"                    : "yes",      "x11_display_offset"                : "10",      "x11_use_localhost"                 : "yes",      "print_motd"                        : "no",      "print_last_log"                    : "yes",      "t_c_p_keep_alive"                  : "yes",      "subsystem"                         : "sftp /usr/lib/openssh/sftp-server",      "use_p_a_m"                         : "yes",      "permit_root_login"                 : "no"    }  },  "_comment_base_setting" : [ "readme file : ./site-cookbooks/base/README.md"],  "base" : {    "dotfilesrepo" : "sanritz"  },  "_comment_vim_setting" : [ "readme file : ./cookbooks/vim/README.md" ],  "vim" : {    "install_method" : "source",    "source" : {      "version" : "7.4",      "dependencies" : [        "gettext",        "libncurses5-dev",        "libacl1-dev",        "libgpm-dev",        "libxmu-dev",        "libgnomeui-dev",        "libxpm-dev",        "libperl-dev",        "python-dev",        "python3-dev",        "ruby-dev",        "lua5.2",        "liblua5.2-dev",        "tcl-dev",        "libluajit-5.1"      ],      "configuration" : [        "--with-features=huge",        "--enable-fail-if-missing",        "--enable-perlinterp",        "--enable-pythoninterp",        "--enable-python3interp",        "--enable-rubyinterp",        "--enable-tclinterp",        "--enable-luainterp",        "--with-lua-prefix=/usr",        "--enable-fontset",        "--enable-multibyte",        "--enable-gpm",        "--enable-gui=gnome2",        "--enable-xim",        "--enable-cscope",        "--with-x",        "--disable-selinux",        "--prefix=/usr/local"      ]    }  },  "nginx" : {    "default_site_enabled" : false  },  "_comment_mysql_setting" : [ "readme file : ./cookbooks/mysql/README.md"],  "mysql": {    "server_root_password"  : "rootpasswd",    "server_repl_password"  : "rootpasswd",    "server_debian_password": "rootpasswd"  },  "gitlab":{    "database_adapter" :"mysql",    "host"             :"172.16.6.10",    "database_password":"datapass",    "port"             :"80",    "max_size"         :"1048576000"  },  "_comment_server_base_setting" : [ "readme file : ./site-cookbooks/server_base/README.md"],  "server_base" : {    "mail_to" : "kitsunai@sanritz.co.jp",    "ntp_server" : "172.16.2.9",    "do_test" : false  }}